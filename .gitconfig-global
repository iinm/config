[init]
	defaultBranch = main

[core]
	trustctime = false
	excludesfile = ~/.gitignore-global
	ignorecase = false

[pull]
	ff = only

[credential]
	helper = store

[merge]
	tool = vimdiff

[mergetool]
	prompt = true

[difftool]
	prompt = false

[diff]
	tool = vimdiff

[rebase]
	instructionFormat = "(%an) %s"
